<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ui.MyLibrary"
    >

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="44dp"
        android:layout_height="45dp"
        app:srcCompat="@drawable/foto_usuario"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/montserrat_extrabold"
        android:text="Sua Biblioteca"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/imageView6"
        app:layout_constraintTop_toTopOf="@id/imageView6"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bar"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="71dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/imageView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@id/imageView12"
        app:layout_constraintTop_toTopOf="@id/imageView6"
        app:srcCompat="@drawable/search"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/vertical_bar_cross"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/imageView6"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="4dp"
        android:src="@drawable/bar_cross"
        app:layout_constraintEnd_toStartOf="@id/imageView13"
        app:layout_constraintTop_toTopOf="@id/imageView6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="50dp"
        android:layout_height="13dp"
        android:text=" Recentes"
        android:textSize="10sp"
        android:textColor="#5F5F5F"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="80dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/imageView8"
        app:layout_constraintStart_toEndOf="@id/imageView14"
        android:layout_marginTop="8dp"
        android:layout_marginStart="4dp"
        />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="14dp"
        android:layout_height="13dp"
        android:src="@drawable/arrow_botton"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="80dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/imageView8"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="47dp"
        android:layout_height="13dp"
        android:text="GenÃªros"
        android:textColor="#5F5F5F"
        android:textSize="10sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="80dp"
        app:layout_constraintTop_toTopOf="@id/textView9"
        app:layout_constraintStart_toEndOf="@id/textView9"
        android:layout_marginStart="16dp"
        />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="12dp"
        android:layout_height="13dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/textView9"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>